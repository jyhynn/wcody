<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lo">
	<select id="Sido_list" resultType="vo.LocationVO">
		select * from sido
	</select>
 
 	<select id="Gugun_list" resultType="vo.LocationVO">
		select * from gugun
	</select> 
	
	<select id="gugun_nx" resultType="java.lang.String" parameterType="java.lang.String">
		select nx from gugun where gugun=#{gugun}
	</select>
	
	<select id="gugun_ny" resultType="java.lang.String" parameterType="java.lang.String">
		select ny from gugun where gugun=#{gugun}
	</select>
</mapper>












